{
    "name": "openapi-steamworks-web-api",
    "version": "1.0.0",
    "description": "An automatically generated OpenAPI spec for the Steamworks Web API",
    "main": "src/index.js",
    "type": "module",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/ceva24/openapi-steamworks-web-api.git"
    },
    "keywords": [
        "steam",
        "steamworks",
        "web",
        "api",
        "openapi",
        "swagger"
    ],
    "author": "ceva24",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/ceva24/openapi-steamworks-web-api/issues"
    },
    "homepage": "https://github.com/ceva24/openapi-steamworks-web-api#readme",
    "scripts": {
        "start": "NODE_ENV=development node src/index.js",
        "dist": "NODE_ENV=production node src/index.js",
        "format": "prettier --write .",
        "lint": "xo",
        "test": "NODE_ENV=test jest --coverage",
        "check": "npm run lint && npm run test",
        "fc": "npm run format && npm run check",
        "update": "npx npm-check-updates -u"
    },
    "dependencies": {
        "@apidevtools/swagger-parser": "^10.1.0",
        "dotenv": "^16.0.3",
        "minimist": "^1.2.8",
        "node-fetch": "^3.3.1"
    },
    "devDependencies": {
        "@babel/preset-env": "^7.20.2",
        "jest": "^29.5.0",
        "jest-fetch-mock": "^3.0.3",
        "prettier": "^2.8.4",
        "xo": "^0.53.1"
    }
}
